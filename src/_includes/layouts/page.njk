{% extends 'layouts/base.njk' %}

{% block content %}
  <main id="main-content" tabindex="-1" class="page">
    <div class="page__header-wrapper">
      <div class="page__inner-wrapper">
        <h1>{{title}}</h1>
      </div>
    </div>

    <div class="wrapper">
      <div class="page__inner-wrapper">
        <div class="page__content">
          {{ content | safe }}
        </div>
      </div>
    </div>
  </main>
{% endblock %}